{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "admin"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "resource/styles.css",
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "resource/font-awesome/css/font-awesome.min.css",
        "resource/vendors/animate.css/animate.min.css",
        "resource/nprogress/nprogress.css",
        "resource/daterangepicker.css",
        "resource/vendors/iCheck/skins/flat/green.css",
        "resource/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css",
        "resource/vendors/jqvmap/dist/jqvmap.min.css",
        "resource/vendors/bootstrap-daterangepicker/daterangepicker.css"
      ],
      "scripts": [
        "resource/vendors/jquery/dist/jquery.min.js",
        "resource/vendors/bootstrap/dist/js/bootstrap.min.js",
        "resource/vendors/fastclick/lib/fastclick.js",
        "resource/vendors/nprogress/nprogress.js",
        "resource/vendors/Chart.js/dist/Chart.min.js",
        "resource/vendors/gauge.js/dist/gauge.min.js",
        "resource/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js",
        "resource/vendors/iCheck/icheck.min.js",
        "resource/vendors/DateJs/date.js",
        "resource/vendors/skycons/skycons.js",
        "resource/vendors/Flot/jquery.flot.js",
        "resource/vendors/Flot/jquery.flot.pie.js",
        "resource/vendors/Flot/jquery.flot.time.js",
        "resource/vendors/Flot/jquery.flot.stack.js",
        "resource/vendors/Flot/jquery.flot.resize.js",
        "resource/vendors/flot.orderbars/js/jquery.flot.orderBars.js",
        "resource/vendors/flot-spline/js/jquery.flot.spline.min.js",
        "resource/vendors/flot.curvedlines/curvedLines.js",
        "resource/vendors/jqvmap/dist/jquery.vmap.js",
        "resource/vendors/jqvmap/dist/maps/jquery.vmap.world.js",
        "resource/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js",
        "resource/vendors/moment/min/moment.min.js",
        "resource/vendors/bootstrap-daterangepicker/daterangepicker.js",
        "resource/vendors/fastclick/lib/fastclick.js",
        "resource/vendors/nprogress/nprogress.js",
        "resource/vendors/validator/validator.js",
        "resource/custom.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.dev.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
